    "windows": [
      {
        "fullscreen": false,
        "minWidth": 820,
        "minHeight": 550,
        "width": 820,
        "height": 600,
        "transparent": true,
        "resizable": true,
        "title": "CV Generator",
        "titleBarStyle": "Overlay",
        "hiddenTitle": true
